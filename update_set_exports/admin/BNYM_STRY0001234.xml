<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update sys_domain="global" table="sys_script"&gt;&lt;sys_script action="INSERT_OR_UPDATE"&gt;&lt;abort_action&gt;false&lt;/abort_action&gt;&lt;access&gt;package_private&lt;/access&gt;&lt;action_delete&gt;false&lt;/action_delete&gt;&lt;action_insert&gt;false&lt;/action_insert&gt;&lt;action_query&gt;false&lt;/action_query&gt;&lt;action_update&gt;true&lt;/action_update&gt;&lt;active&gt;true&lt;/active&gt;&lt;add_message&gt;false&lt;/add_message&gt;&lt;advanced&gt;true&lt;/advanced&gt;&lt;change_fields&gt;false&lt;/change_fields&gt;&lt;client_callable&gt;false&lt;/client_callable&gt;&lt;collection&gt;change_request&lt;/collection&gt;&lt;condition/&gt;&lt;description/&gt;&lt;execute_function&gt;false&lt;/execute_function&gt;&lt;filter_condition table="change_request"&gt;start_dateVALCHANGES^EQ&lt;item endquery="false" field="start_date" goto="false" newquery="false" operator="VALCHANGES" or="false" value=""/&gt;&lt;item endquery="true" field="" goto="false" newquery="false" operator="=" or="false" value=""/&gt;&lt;/filter_condition&gt;&lt;is_rest&gt;false&lt;/is_rest&gt;&lt;message/&gt;&lt;name&gt;Run LTE Rule&lt;/name&gt;&lt;order&gt;100&lt;/order&gt;&lt;priority&gt;100&lt;/priority&gt;&lt;rest_method/&gt;&lt;rest_method_text/&gt;&lt;rest_service/&gt;&lt;rest_service_text/&gt;&lt;rest_variables/&gt;&lt;role_conditions/&gt;&lt;script&gt;&lt;![CDATA[(function executeRule(current, previous /*null when async*/) {
	var CHANGE_STATE_NEW = '-5';
	// Testing
	gs.getSession().putClientData('tracker_' + current.getUniqueValue(), '');
	var startDateChanged = (current.start_date &amp;&amp; current.start_date != previous.start_date);
	//check should only occur in the NEW state.
	var validState = current.state == CHANGE_STATE_NEW;
	var runInBackground;

	if(previous.start_date != '') {
		// Previous Date plus 2 days to check if date is within 48 hours of previous date.
		var previousDateFuture = new GlideDateTime(previous.start_date);
		previousDateFuture.addDays(2);

		var previousDate = new GlideDateTime(previous.start_date);
		// Should be within previous date and +2 days
		var currentDate = new GlideDateTime(current.start_date);
		var isWithinTwoDays = currentDate.before(previousDateFuture) &amp;&amp; currentDate.after(previousDate);
		runInBackground = validState &amp;&amp; startDateChanged &amp;&amp; (isWithinTwoDays == false);
	}
	
	if(previous.start_date == '') {
		runInBackground = validState &amp;&amp; startDateChanged;
	}
	
	gs.addInfoMessage('Run In Background: ' + runInBackground);

	if (runInBackground) {
		var worker = new GlideScriptedHierarchicalWorker();
		worker.setProgressName(gs.getMessage("Lead time detection"));
		worker.setScriptIncludeName("ChangeLeadTimeConflictWorker");
		worker.setScriptIncludeMethod("start");
		worker.putMethodArg("sysId", current.getUniqueValue());
		worker.setBackground(true);
		worker.start();
	}

})(current, previous);]]&gt;&lt;/script&gt;&lt;sys_class_name&gt;sys_script&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2021-09-20 21:54:21&lt;/sys_created_on&gt;&lt;sys_domain&gt;global&lt;/sys_domain&gt;&lt;sys_domain_path&gt;/&lt;/sys_domain_path&gt;&lt;sys_id&gt;98d456dc2f76301069ecdcb6f699b6a5&lt;/sys_id&gt;&lt;sys_mod_count&gt;26&lt;/sys_mod_count&gt;&lt;sys_name&gt;Run LTE Rule&lt;/sys_name&gt;&lt;sys_overrides/&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_script_98d456dc2f76301069ecdcb6f699b6a5&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2021-11-21 03:10:41&lt;/sys_updated_on&gt;&lt;template/&gt;&lt;when&gt;before&lt;/when&gt;&lt;/sys_script&gt;&lt;sys_translated_text action="delete_multiple" query="documentkey=98d456dc2f76301069ecdcb6f699b6a5"/&gt;&lt;/record_update&gt;</payload>
